{
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all test eslint stylelint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "eslint": "npx eslint .",
    "eslint-fix": "npx eslint . --fix",
    "stylelint": "npx stylelint **/*.scss",
    "stylelint-fix": "npx stylelint **/*.scss --fix",
    "prebuild": "tsc --noEmit",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite build && node server.js",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register --require ./mochaSetup.cjs './src/**/*.test.ts'"
  },
  "devDependencies": {
    "@types/chai": "4.3.11",
    "@types/jsdom": "21.1.6",
    "@types/mocha": "10.0.6",
    "@types/sinon": "17.0.2",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "chai": "4.3.10",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "mocha": "10.2.0",
    "sass": "1.67.0",
    "sinon": "17.0.1",
    "stylelint": "15.10.3",
    "stylelint-config-standard-scss": "11.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.4.11"
  },
  "dependencies": {
    "eslint-plugin-react": "7.33.2",
    "express": "4.18.2",
    "handlebars": "4.7.8",
    "nanoid": "3.0.0",
    "vite-plugin-handlebars": "1.6.0"
  },
  "engines": {
    "node": "18.17.1"
  }
}
